{
  "name": "tic-tac-toe-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "vercel-build": "npm install @rollup/rollup-linux-x64-gnu && npm run build"
  },
  "dependencies": {
    "@google/genai": "^1.8.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  },
  "overrides": {
    "rollup": {
      "@rollup/rollup-linux-x64-gnu": "4.13.0"
    }
  }
}